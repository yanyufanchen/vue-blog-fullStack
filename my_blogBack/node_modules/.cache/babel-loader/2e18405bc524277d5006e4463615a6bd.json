 自定义色提交\n    customClick: function customClick() {\n      console.log(this.deskTheme, '选择的颜色');\n      this.ascertain(this.deskTheme);\n    },\n    // 提交弹框\n    ascertain: function ascertain(color) {\n      var _this = this;\n\n      console.log(color, '选择的颜色');\n      this.$confirm('要修改前台主题色吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(function () {\n        _this.$message({\n          type: 'success',\n          message: '修改成功!'\n        });\n      }).catch(function () {\n        _this.$message({\n          type: 'info',\n          message: '已取消修改'\n        });\n      });\n    }\n  },\n  mounted: function mounted() {}\n};",{"version":3,"sources":["DeskSet.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyEA,OAAA,OAAA;AACA,OAAA,MAAA,MAAA,QAAA,C,CAAA;;AACA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,CAAA,MAAA,CADA;AACA;AACA,MAAA,WAAA,EAAA,IAFA;AAGA,MAAA,SAAA,EAAA,CACA,SADA,EAEA,SAFA,EAGA,SAHA,EAIA,SAJA,EAKA,SALA,EAMA,SANA,EAOA,SAPA,EAQA,SARA,EASA,wBATA,EAUA,kBAVA,EAWA,WAXA,CAHA;AAgBA,MAAA,SAAA,EAAA,EAhBA,CAgBA;;AAhBA,KAAA;AAkBA,GApBA;AAqBA,EAAA,OArBA,qBAqBA;AACA,SAAA,SAAA,GAAA,MAAA,CADA,CACA;AACA,GAvBA;AAwBA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,UAFA,sBAEA,CAFA,EAEA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,MAAA,CAAA,OAAA,CAAA,KAAA,EAAA,OAAA;AACA,WAAA,SAAA,CAAA,CAAA,CAAA,MAAA,CAAA,OAAA,CAAA,KAAA;AACA,KALA;AAMA;AACA,IAAA,WAPA,yBAOA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA,EAAA,OAAA;AACA,WAAA,SAAA,CAAA,KAAA,SAAA;AACA,KAVA;AAWA;AACA,IAAA,SAZA,qBAYA,KAZA,EAYA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,OAAA;AACA,WAAA,QAAA,CAAA,YAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OATA,EASA,KATA,CASA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAA